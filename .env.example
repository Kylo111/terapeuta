# Konfiguracja serwera
PORT=3000
NODE_ENV=development

# Konfiguracja bazy danych MongoDB
MONGODB_URI=mongodb://localhost:27017/terapeuta
MONGODB_URI_PROD=mongodb+srv://username:password@cluster.mongodb.net/terapeuta

# Klucze JWT
JWT_SECRET=your_jwt_secret_key
JWT_REFRESH_SECRET=your_jwt_refresh_secret_key
JWT_EXPIRES_IN=7d

# Konfiguracja API modeli LLM
OPENAI_API_KEY=your_openai_api_key
ANTHROPIC_API_KEY=your_anthropic_api_key
GOOGLE_API_KEY=your_google_api_key
HUGGINGFACE_API_KEY=your_huggingface_api_key

# Konfiguracja LLM
DEFAULT_LLM_PROVIDER=openai
DEFAULT_LLM_MODEL=gpt-4o

# Konfiguracja powiadomień e-mail
EMAIL_HOST=smtp.gmail.com
EMAIL_PORT=587
EMAIL_SECURE=false
EMAIL_USER=your_email@gmail.com
EMAIL_PASSWORD=your_app_password
EMAIL_FROM=your_email@gmail.com

# Konfiguracja frontendu
FRONTEND_URL=http://localhost:3000

# Konfiguracja Supabase
SUPABASE_URL=your_supabase_url
SUPABASE_KEY=your_supabase_key

# Konfiguracja usług zewnętrznych
SENTRY_DSN=your_sentry_dsn
UMAMI_WEBSITE_ID=your_umami_website_id
UMAMI_URL=your_umami_url
